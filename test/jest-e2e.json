{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@modules/(.*)$": "<rootDir>/src/app/modules/$1",
    "^@core/(.*)$": "<rootDir>/src/app/core/$1",
    "^@config/(.*)$": "<rootDir>/src/app/config/$1"
  }
}
