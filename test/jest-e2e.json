{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@core/(.*)$": "<rootDir>/src/app/core/$1",
    "^@modules/(.*)$": "<rootDir>/src/app/modules/$1"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/setup/e2e.setup.ts"]
}
